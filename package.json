  "engines": {
    "node": ">=16"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "react",
        "react-dom",
        "@babel/*",
        "webpack",
        "@storybook/*",
        "@types/react",
        "eslint"
      ]
    }
  },
  "npm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "react",
        "react-dom",
        "@babel/*",
        "webpack",
        "@storybook/*"
      ]
    }
  },
  "msw": {
    "workerDirectory": "public"
  }
}