{
  "name": "surreal-db",
  "version": "0.1.0",
  "private": true,
  "description": "Solid Start, TanStack Query and Tailwind CSS with CSS Modules",
  "keywords": [
    "solidstart",
    "tanstack-query",
    "tailwind",
    "storybook",
    "typescript",
    "vite",
    "mock-service-worker",
    "solidjs",
    "vitest",
    "css module"
  ],
  "showcases": [
    {
      "kit": "solidstart-tanstackquery-tailwind-modules",
